{
  "hosting": [{
    "target": "rxresume-staging",
    "public": "public",
    "ignore": [
      "**/.*",
      "firebase.json",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/printSinglePageResume",
        "function": "printSinglePageResume"
      },
      {
        "source": "/printMultiPageResume",
        "function": "printMultiPageResume"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }],
  "emulators": {
    "functions": {
      "port": 5001
    },
    "hosting": {
      "port": 5000
    },
    "ui": {
      "enabled": true
    }
  },
  "functions": {
    "source": "functions"
  }
}
